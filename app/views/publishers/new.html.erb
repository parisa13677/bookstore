<h2> Add New Publisher</h2>
<%= form_for @publisher do |f|%>
     <div class="from-group">
       <%= f.label :name %><br />
       <%= f.text_field :name, class: 'from-control' %><br />
  </div>

  <%= f.submit "submit", class:'btn btn-primary'%>
  <%=link_to 'Cancel', publishers_path, class:'btn btn-default'%>
<% end %>
